import React from 'react';

const VideoList = ({ videos }) => {
  return (
    <div>
      <h2>Video List</h2>
      {videos.map((video) => (
        <div key={video._id}>
          <iframe
            width="560"
            height="315"
            src={`https://www.youtube.com/embed/${video.videoId}`}
            title={video.title}
            frameBorder="0"
            allowFullScreen
          ></iframe>
        </div>
      ))}
    </div>
  );
};

export default VideoList;